{
  "manifest_version": 2,

  "name": "InstaLinkShareify",
  "description": "Send a link to a Facebook friend with just one click!",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png"
  }
}